{
  "name": "serverless-swagger-ui",
  "version": "1.0.0",
  "description": "Self-hosted serverless Swagger UI for AWS API Gateway",
  "author": "Maciej Radzikowski <maciej@radzikowski.com.pl>",
  "license": "MIT",
  "scripts": {
    "deploy": "AWS_PROFILE=elefam-dev serverless deploy --region us-east-1 --aws-profile elefam-dev",
    "remove": "AWS_PROFILE=elefam-dev  serverless remove  --region us-east-1 --aws-profile elefam-dev",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "@aws-amplify/auth": "3.4.6",
    "swagger-ui-dist": "3.35.0"
  },
  "devDependencies": {
    "@types/swagger-ui-dist": "3.30.0",
    "aws-sdk": "2.770.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.3.0",
    "html-webpack-plugin": "4.5.0",
    "robotstxt-webpack-plugin": "7.0.0",
    "serverless": "^4.7.0",
    "serverless-cloudformation-sub-variables": "^0.2.1",
    "serverless-s3-deploy": "^0.10.1",
    "serverless-s3-remover": "^0.6.0",
    "serverless-scriptable-plugin": "^1.3.1",
    "style-loader": "1.3.0",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
